        <SQLTask:SqlTaskData
          SQLTask:Connection="{E3346F67-D434-448F-A576-C61D167694DC}"
          SQLTask:SqlStatementSource="declare @FechaInicio datetime&#xA;declare @FechaTermino datetime&#xA;declare @LoopFecha datetime&#xA;&#xA;set @FechaInicio ='01/01/2014' -- mes/dia/annio&#xA;set @FechaTermino = GETDATE()  -- fecha del dia de hoy &#xA;set @LoopFecha=@FechaInicio&#xA;&#xA;while @LoopFecha&lt;= @FechaTermino&#xA;begin&#xA;  INSERT INTO [Dim_School_All_in_One].[dbo].[dim_tiempo]&#xA;           (&#xA;&#x9;[id_tiempo_pks],&#xA;&#x9;[mes],&#xA;&#x9;[trimestre],&#xA;&#x9;[semestre],&#xA;&#x9;[annio],&#xA;&#x9;[nomb_mes]&#xA;&#x9;&#x9;   )&#xA;     VALUES&#xA;           (&#xA;&#x9;cast(convert(varchar(8),@LoopFecha,112) as int),   --YYYYMMDD&#xA;&#x9;MONTH(@LoopFecha),      -- mes&#xA;&#x9;DATEPART(qq,@LoopFecha), -- trimestre&#xA;&#x9;case when MONTH(@LoopFecha) in(1,2,3,4,5,6) then 1&#xA;&#x9;         when MONTH(@LoopFecha) in (7,8,9,10,11,12) then 2&#xA;&#x9;end,&#x9;-- semestre&#xA;&#x9;YEAR(@LoopFecha),&#x9;-- año&#xA;&#x9;DATENAME(month, @LoopFecha) -- nombre del mes&#xA;&#x9;)&#xA;&#xA;    set @LoopFecha =DATEADD(dd,1,@LoopFecha)&#xA;&#xA;end" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
